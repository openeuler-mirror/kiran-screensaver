<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="com.kylinsec.kiran.screensaver" path="/com/kylinsec/kiran/screensaver/">
        <key name="screensaver-locker" type="s">
            <default>"/usr/lib64/kiran-screensaver/libkiran-screensaver-dialog.so"</default>
        </key>

<!--        <key name="idle-activation-lock" type="b">-->
<!--            <default>true</default>-->
<!--            <description>空闲时是否锁定</description>-->
<!--        </key>-->

        <key name="can-logout" type="b">
            <default>true</default>
            <description>是否允许登出</description>
        </key>

        <key name="can-user-switch" type="b">
            <default>true</default>
            <description>是否允许切换用户</description>
        </key>

        <key name="enable-animation" type="b">
            <default>true</default>
            <description>是否启用动画</description>
        </key>
    </schema>
</schemalist>
